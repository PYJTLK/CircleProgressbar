<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <attr name="proressColor" format="color"/>
        <attr name="progressBarWidth" format="dimension"/>
        <attr name="cacheColor" format="color"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="progress" format="float"/>
        <attr name="cacheProgress" format="float"/>
        <attr name="style" format="enum">
            <enum name="arc" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr name="progressBackgroundColor" format="color"/>
        <attr name="hasGravity" format="boolean"/>
    </declare-styleable>
</resources>